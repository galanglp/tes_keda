<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="kedatech.list">
      <field name="name">kedatech list</field>
      <field name="model">kedatech.kedatech</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="kedatech.action_window">
      <field name="name">kedatech window</field>
      <field name="res_model">kedatech.kedatech</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="kedatech.action_server">
      <field name="name">kedatech server</field>
      <field name="model_id" ref="model_kedatech_kedatech"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="kedatech" id="kedatech.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="kedatech.menu_1" parent="kedatech.menu_root"/>
    <menuitem name="Menu 2" id="kedatech.menu_2" parent="kedatech.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="kedatech.menu_1_list" parent="kedatech.menu_1"
              action="kedatech.action_window"/>
    <menuitem name="Server to list" id="kedatech" parent="kedatech.menu_2"
              action="kedatech.action_server"/>

  </data>
</odoo>